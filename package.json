  "main": "umd/react-tweened.js",
  "module": "es/react-tweened.js",
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npm run build:module && npm run build:umd && npm run build:demo",
    "build:demo": "next build",
    "build:umd": "webpack --config webpack.config.babel.js",
    "build:module": "babel -d es src",
    "dev": "next",
    "now-build": "next build",
    "now-start": "next start",
    "postpublish": "now -t $NOW_TOKEN; now alias",
    "pretest": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "ava",
    "watch": "webpack --watch --config webpack.config.babel.js"
  },
  "dependencies": {
    "debug": "^2.6.0"
  },
  "peerDependencies": {
    "react": ""
  },
  "devDependencies": {
    "@amonks/webpack-helpers": "2.0.0",
    "ava": "^0.18.1",
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "cracks": "^3.1.2",
    "enzyme": "^2.7.1",
    "next": "2",
    "now": "^4.3.0",
    "npm-run-all": "^4.0.1",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "semantic-release": "^6.3.2",
    "serve": "^3.2.10",
    "webpack": "^2.2.1"
